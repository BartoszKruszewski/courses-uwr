@model WebApplication1.Models.TaskModel

@{
    ViewData["Title"] = "Results";
}

<h2>Submitted Tasks</h2>

<p><strong>First Name:</strong> @Model.FirstName</p>
<p><strong>Last Name:</strong> @Model.LastName</p>

<h3>Tasks</h3>
<ul>
    @for (int i = 0; i < 10; i++)
    {
        <li>@Model.Tasks[i]</li>
    }
</ul>
